<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.sample_with_android"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10"/>

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE"/>

    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="LoginForm"
                  android:label="@string/login.form">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="ShopForm" android:label="@string/shop.form"/>
        <activity android:name="Date" android:label="@string/date"/>
        <activity android:name="Time" android:label="@string/time"/>
        <activity android:name="Calendar" android:label="@string/calendar"/>
        <activity android:name="ListData" android:label="@string/list"/>
        <activity android:name="com.example.activity.Widget" android:label="@string/widgets"/>
    </application>
</manifest>
